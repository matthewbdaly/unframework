{
    "name": "matthewbdaly/unframework",
    "description": "Not a framework. An opinionated collection of PHP libraries and some simple glue code binding them together.",
    "type": "project",
    "require": {
        "symfony/http-foundation": "^5.0",
        "symfony/console": "^5.0",
        "twig/twig": "^3.0",
        "monolog/monolog": "^2.0",
        "league/event": "^2.1",
        "laminas/laminas-diactoros": "^2.2",
        "doctrine/orm": "^2.6",
        "vlucas/phpdotenv": "^4.1",
        "league/flysystem": "^1.0",
        "filp/whoops": "^2.2",
        "league/route": "^4.1",
        "league/container": "^3.2",
        "laminas/laminas-httphandlerrunner": "^1.0",
        "doctrine/cache": "^1.10",
        "cache/doctrine-adapter": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3",
        "psy/psysh": "^0.10.3",
        "php-coveralls/php-coveralls": "^2.2",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.11"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
